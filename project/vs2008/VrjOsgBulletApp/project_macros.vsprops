<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="project_macros"
	>
	<UserMacro
		Name="PROJECT_ROOT"
		Value="..\.."
	/>
	<UserMacro
		Name="PROJECT_SRC"
		Value="$(PROJECT_ROOT)\src"
	/>
	<UserMacro
		Name="PROJECT_LIB"
		Value="$(PROJECT_ROOT)\lib"
	/>
	<UserMacro
		Name="PROJECT_BIN"
		Value="$(PROJECT_ROOT)\bin"
	/>
</VisualStudioPropertySheet>
